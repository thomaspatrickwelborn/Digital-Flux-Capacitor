<%# BLOCK -%>
<%# BLOCK STATEMENT -%>
<%- await include('statement/index.ejs', $data) -%>
<%# BLOCK ELEMENT -%>
<%
const { coutils } = $data
const {
  beautify, ejs, path,
  renderFileOptions, beautifyFileOptions,
  templateDir
} = coutils
const templatePath = path.join(
	templateDir, 'blocks/block/element/index.ejs'
)
const templateRender = await ejs.renderFile(
	templatePath, $data, renderFileOptions
)
const beautifiedTemplateRender = beautify.html(templateRender, beautifyFileOptions)
-%>
<%- beautifiedTemplateRender %>