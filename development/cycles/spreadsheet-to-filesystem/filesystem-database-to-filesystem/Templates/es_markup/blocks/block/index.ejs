<%# BLOCK -%>
<%# BLOCK STATEMENT -%>
<%- await include('statement/index.ejs', $data) -%>
<%# BLOCK ELEMENT -%>
<%
const { coutils } = $data
const {
  beautify, ejs, path,
  renderFileOptions, beautifyFileOptions,
  templateDir
} = coutils
const elementTemplatePath = path.join(
	templateDir, 'blocks/block/element/index.ejs'
)
const elementTemplateRender = await ejs.renderFile(
	elementTemplatePath, $data, renderFileOptions
)
const beautifiedElementTemplateRender = beautify.html(elementTemplateRender, beautifyFileOptions)
-%>
<%- beautifiedElementTemplateRender _%>