<%_
// BLOCK
const { coutils } = $data
const {
  beautify, ejs, path,
  renderFileOptions, beautifyFileOptions,
  templateDir
} = coutils
// BLOCK STATEMENT
const statementTemplatePath = path.join(
  templateDir, 'blocks/block/statement/index.ejs'
)
const statementTemplateRender = await ejs.renderFile(
  statementTemplatePath, $data, renderFileOptions
)
const beautifiedStatementTemplateRender = beautify.js(
  statementTemplateRender, beautifyFileOptions
).trim()
_%>
<% console.log('beautifiedStatementTemplateRender', beautifiedStatementTemplateRender) %>
<%- beautifiedStatementTemplateRender _%>
<%_
// BLOCK ELEMENT
const elementTemplatePath = path.join(
  templateDir, 'blocks/block/element/index.ejs'
)
const elementTemplateRender = await ejs.renderFile(
  elementTemplatePath, $data, renderFileOptions
)
const beautifiedElementTemplateRender = beautify.html(
  elementTemplateRender, beautifyFileOptions
).trim()
_%>
<%- beautifiedElementTemplateRender _%>
