<%# BLOCK -%>
<%
const { coutils, content } = $data
const { blocks, statement } = content
const { lexter, dexter } = statement
-%>
<%# STATEMENT MEXTER SCOPE -%>
<%
const commexter = [lexter, dexter]
var commexterIndex = 0
-%>
<% while(commexterIndex < commexter.length) { -%>
  <%
  const mexter = commexter[commexterIndex]
  var { ser, ten, per, pos } = mexter
  -%>
  <% if(
    ten &&
    ten.slice(0, 2) !== '#:') {
  -%>
		<%= ser %> <%- ten %> <%= per %>
  <% } else { -%>
  	<%= ser %> <%= per %>
  <% } -%>
  <% if(pos !== undefined) { -%>
  	<% if(
	  	commexterIndex === 0 &&
	  	commexter[1].pos !== undefined
  	) { -%>
  		<%= pos -%>
  		<%
  		pos = commexter[1].pos
  		commexterIndex++
  		-%>
  	<% } -%>
	  <% const [inpos, expos] = pos || [] -%>
	  <%# INPOS %>
		<%= inpos -%>
		<%# BLOCKS %>
		<% if(blocks.length) { -%>
			<%- await include('../index.ejs', {
				content: blocks,
				coutils: coutils,
			}) %>
		<% } -%>
	  <%# EXPOS -%>
		<%= expos -%>
  <% } -%>
  <% commexterIndex++ -%>
<% } -%>