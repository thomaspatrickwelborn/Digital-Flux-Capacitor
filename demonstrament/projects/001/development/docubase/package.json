{
  "name": "dfc-configurator-docubase",
  "version": "1.0.0",
  "author": "Thomas Patrick Welborn",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "livereload": "^0.9.3",
    "connect-livereload": "^0.6.1"
  },
  "devDependencies": {
    "pm2": "^5.4.1"
  },
  "scripts": {
    "save": "npx pm2 save",
    "start": "npx pm2 start ecosystem.config.cjs && npm run save",
    "restart": "npx pm2 restart ecosystem.config.cjs",
    "stop": "npx pm2 stop ecosystem.config.cjs && npm run save",
    "delete": "npx pm2 delete ecosystem.config.cjs && npm run save",
    "ls": "npx pm2 ls"
  }
}